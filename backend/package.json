{
  "name": "vitalrack-backend",
  "version": "1.0.0",
  "description": "API de Vitalrack: Express + TypeScript + MongoDB",
  "main": "dist/index.js",
  "scripts": {
    "start": "nodemon --watch src --ext ts --exec \"ts-node src/index.ts\"",
    "build": "tsc",
    "lint": "eslint \"src/**/*.ts\"",
    "test": "jest --coverage",
    "migrate": "ts-node src/migrations/initIndexes.ts"
  },
  "dependencies": {
    "axios": "^1.8.4",
    "bcryptjs": "^2.4.3",
    "dotenv": "^10.0.0",
    "express": "^4.17.3",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^6.5.2"
  },
  "devDependencies": {
    "@types/axios": "^0.9.36",
    "@types/bcryptjs": "^2.4.2",
    "@types/express": "^4.17.21",
    "@types/jest": "^27.5.2",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/mongoose": "^5.11.96",
    "@types/node": "^17.0.45",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^8.31.0",
    "@typescript-eslint/parser": "^8.31.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^28.1.0",
    "nodemon": "^3.1.9",
    "prettier": "^2.6.2",
    "supertest": "^6.2.3",
    "ts-jest": "^28.0.3",
    "ts-node": "^10.9.2",
    "typescript": "^4.6.3"
  }
}
